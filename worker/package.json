{
  "name": "worker",
  "version": "1.0.0",
  "description": "Cloudflare worker for deno.land",
  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "dev": "NODE_ENV=development npm run build",
    "format": "prettier --write '**/*.{ts,js,css,json,md}'",
    "test:clean": "rimraf ./transpiled/shared ./transpiled/worker",
    "test": "npm run test:clean && npm run transpile && mocha --require source-map-support/register --recursive transpiled/worker/test",
    "transpile": "tsc --project ./test"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^1.0.1",
    "@dollarshaveclub/cloudworker": "^0.1.2",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.7.7",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.12",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6"
  }
}
